{
  "name": "services-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../../.env tsx watch src/index.ts",
    "build": "prisma generate --schema ./prisma/schema.prisma && tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "start": "DOTENV_CONFIG_PATH=../../.env node dist/index.js",
    "lint": "biome check src",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "@supabase/supabase-js": "2.45.3",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "resend": "3.2.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.10.6",
    "@types/cors": "2.8.17",
    "@types/jsonwebtoken": "9.0.6",
    "@found-poems/tsconfig": "workspace:*",
    "prisma": "5.20.0",
    "rimraf": "6.0.1",
    "tsup": "8.3.0",
    "tsx": "4.19.2",
    "typescript": "5.6.3"
  }
}
